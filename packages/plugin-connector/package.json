{
	"name": "@evidence-dev/plugin-connector",
	"version": "2.0.0-usql.21",
	"description": "",
	"type": "module",
	"bin": "./dist/debugger/cli.js",
	"scripts": {
		"test": "vitest --run",
		"test:watch": "vitest",
		"build": "tsc --noEmit -p tsconfig.json && parcel build",
		"build:debug": "parcel build --no-optimize",
		"watch": "parcel watch",
		"check": "tsc --noEmit -p tsconfig.json",
		"prepublish": "tsc --noEmit -p tsconfig.json && parcel build"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@evidence-dev/db-commons": "workspace:*",
		"@evidence-dev/universal-sql": "workspace:*",
		"@types/estree": "^1.0.1",
		"@types/lodash.merge": "^4.6.8",
		"chalk": "^5.2.0",
		"chokidar": "^3.5.3",
		"listr2": "^7.0.2",
		"lodash.merge": "^4.6.2",
		"ora": "^7.0.1",
		"svelte": "3.55.0",
		"svelte-preprocess": "^5.0.3",
		"sveltekit-autoimport": "^1.7.0",
		"yaml": "^2.2.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@parcel/core": "^2.8.3",
		"@parcel/packager-ts": "^2.8.3",
		"@parcel/transformer-inline-string": "^2.8.3",
		"@parcel/transformer-typescript-types": "^2.8.3",
		"@types/mock-fs": "^4.13.1",
		"@types/node": "^20.1.2",
		"commander": "^11.0.0",
		"mock-fs": "^5.2.0",
		"parcel": "^2.8.3",
		"typescript": "^5.0.4",
		"vitest": "^0.34.1"
	},
	"targets": {
		"types": false,
		"main": {
			"context": "node"
		},
		"module": {
			"context": "node"
		},
		"debugger": {
			"context": "node",
			"source": "./src/cli.js",
			"outputFormat": "esmodule"
		},
		"loadConfig": {
			"context": "node",
			"source": "./src/plugin-discovery/load-config.js",
			"outputFormat": "commonjs",
			"optimize": false,
			"isLibrary": true,
			"sourceMap": false
		}
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./load-config": "./dist/loadConfig/load-config.js"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"source": "src/index.js",
	"types": "./types/index.d.ts"
}
