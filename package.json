{
	"devDependencies": {
		"@changesets/cli": "2.21.0",
		"@evidence-dev/components": "link:packages/components",
		"@evidence-dev/db-orchestrator": "link:packages/db-orchestrator",
		"@evidence-dev/evidence": "link:packages/evidence",
		"@evidence-dev/preprocess": "link:packages/preprocess",
		"@evidence-dev/telemetry": "link:packages/telemetry",
		"@sveltejs/adapter-static": "1.0.0",
		"@sveltejs/kit": "1.13.0",
		"@tidyjs/tidy": "2.4.4",
		"blueimp-md5": "2.19.0",
		"debounce": "^1.2.1",
		"downloadjs": "1.4.7",
		"echarts": "5.3.2",
		"echarts-stat": "1.2.0",
		"eslint": "^8.36.0",
		"export-to-csv": "0.2.1",
		"fs-extra": "10.1.0",
		"git-remote-origin-url": "4.0.0",
		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.7.0",
		"prismjs": "1.29.0",
		"remark-parse": "8.0.2",
		"ssf": "^0.11.2",
		"svelte": "3.55.0",
		"svelte-icons": "2.1.0",
		"svelte-preprocess": "5.0.3",
		"svelte-tiny-linked-charts": "1.1.5",
		"svelte2tsx": "0.6.0",
		"typescript": "4.9.5",
		"unified": "9.1.0",
		"unist-util-visit": "4.1.2",
		"uvu": "0.5.2",
		"vite": "4.0.4"
	},
	"scripts": {
		"dev": "pnpm --filter ./sites/example-project dev",
		"build:dev-workspace": "pnpm --filter ./sites/example-project build",
		"build:production": "VITE_BUILD_STRICT=true pnpm --filter ./sites/example-project build",
		"build:docs": "pnpm --filter ./sites/docs build",
		"release": "pnpm --filter ./sites/example-project package && pnpm --filter ./packages/evidence build && pnpm changeset publish",
		"release:next": "VERSION=$(git rev-parse --short HEAD) && pnpm -r exec npm pkg set version=0.0.0-\"$VERSION\" && pnpm install --frozen-lockfile && pnpm -r publish --tag next --no-git-checks",
		"test": "pnpm --filter ./sites/example-project package && pnpm --filter ./packages/evidence build && pnpm install --frozen-lockfile && pnpm -r test",
		"test:ui": "pnpm -r test:ui",
		"postinstall": "pnpm --filter ./packages/preprocess build && pnpm --filter ./sites/example-project package && pnpm --filter ./packages/evidence build && pnpm install --ignore-scripts ",
		"docs": "pnpm --filter ./sites/docs start"
	},
	"pnpm": {
		"overrides": {
			"got@<11.8.5": ">=11.8.5",
			"jsonwebtoken@<=8.5.1": ">=9.0.0",
			"trim@<0.0.3": ">=0.0.3",
			"undici@<5.19.1": ">=5.19.1",
			"undici@>=2.0.0 <5.19.1": ">=5.19.1",
			"jsonwebtoken@<9.0.0": ">=9.0.0",
			"nth-check@<2.0.1": ">=2.0.1"
		}
	}
}
